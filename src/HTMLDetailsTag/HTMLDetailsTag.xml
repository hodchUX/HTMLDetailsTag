<?xml version="1.0" encoding="utf-8" ?>
<widget id="HTMLDetailsTag.widget.HTMLDetailsTag" needsEntityContext="false" xmlns="http://www.mendix.com/widget/1.0/">
    <name>HTMLDetailsTag</name>
    <description>The description of this widget.</description>
    <icon></icon>

    <properties>
        <property key="header" type="string" required="true" defaultValue="">
            <caption>Header</caption>
            <category>Appearance</category>
            <description>The header is used to specify a visible heading for the details. The heading can be clicked to view/hide the details.</description>
        </property>
        <property key="applyClass" type="string" required="false" defaultValue="">
            <caption>Content Class</caption>
            <category>Appearance</category>
            <description>Apply a class to the content div.</description>
        </property>
        <property key="contents" type="translatableString" multiline="true" required="false">
            <caption>Contents</caption>
            <category>Appearance</category>
            <description>The contents of the details to hide/show.</description>
        </property>
    </properties>
</widget>

